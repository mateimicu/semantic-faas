@prefix : <http://www.semantic-faas.com/ontology#> .
@prefix dave: <http://theme-e.adaptcentre.ie/dave#> .
@prefix ns1: <http://purl.org/dc/terms/> .
@prefix ns2: <http://xmlns.com/foaf/0.1/> .
@prefix ns3: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix wd: <http://www.wikidata.org/entity/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

dave:Metric owl:equivalentClass :Metric .

<http://www.semantic-faas.com/ontology> a owl:Ontology ;
    ns3:description "Framework for describing Function as a Sevice providers and application requirements that can take advantage of a FaaS provider."@en ;
    ns1:creator "Matei-Marius Micu <micumatei@gmail.com>"@en,
        "Stefan Gordin <stefangordindev@gmail.com>"@en ;
    ns1:title "Function as a Service Ontology"@en ;
    owl:imports dave: ;
    owl:versionIRI <http://www.semantic-faas.com/ontology/0.0.1> ;
    owl:versionInfo "0.0.1" ;
    ns2:homepage "semantic-faas.com"@en ;
    ns2:name "Function as a Service Ontology"@en .

:AWSLambda a :FaaSExecutionEnvironment,
        owl:NamedIndividual ;
    rdfs:label "AWS Lambda"^^xsd:string ;
    :cpuArch wd:Q272629 ;
    rdfs:comment "https://aws.amazon.com/lambda/"^^xsd:anyURI .

:Metrics a owl:AsymmetricProperty,
        owl:IrreflexiveProperty,
        owl:ObjectProperty ;
    rdfs:domain :FaaSExecutionEnvironment ;
    rdfs:range :Metric .

<http://www.semantic-faas.com/ontology#SLA's> a owl:AsymmetricProperty,
        owl:IrreflexiveProperty,
        owl:ObjectProperty ;
    rdfs:domain :Application,
        :FaaSExecutionEnvironment ;
    rdfs:range :SLA .

:availableRegions a owl:AsymmetricProperty,
        owl:IrreflexiveProperty,
        owl:ObjectProperty ;
    rdfs:label "supportedRegions"@en ;
    rdfs:domain :FaaSExecutionEnvironment .

:canIntegrateWith a owl:ObjectProperty ;
    rdfs:comment "What software can be integrated with this execution environment."@en ;
    rdfs:domain :FaaSExecutionEnvironment .

:code a owl:AsymmetricProperty,
        owl:IrreflexiveProperty,
        owl:ObjectProperty ;
    rdfs:comment "Source Code of the applicaiton"@en ;
    rdfs:domain :Application ;
    rdfs:range :Code .

:cpuArch a owl:AsymmetricProperty,
        owl:IrreflexiveProperty,
        owl:ObjectProperty ;
    rdfs:comment "CPU Architectures required for the code to work"@en ;
    rdfs:domain :CPURequirement,
        :FaaSExecutionEnvironment ;
    rdfs:range wd:Q272683 .

:geoRestrictions a owl:AsymmetricProperty,
        owl:IrreflexiveProperty,
        owl:ObjectProperty ;
    rdfs:domain :GeographicalRequirement .

:maxAllowedRAM a owl:DatatypeProperty ;
    rdfs:comment "Maximum allowed RAM comsumtion"@en ;
    rdfs:domain :MemoryRequirement ;
    rdfs:range xsd:positiveInteger .

:maxAllowedStorage a owl:DatatypeProperty ;
    rdfs:comment "Maximum storage allowed to be used."@en ;
    rdfs:domain :StorageRequirement ;
    rdfs:range xsd:positiveInteger .

:maxConccurency a owl:DatatypeProperty ;
    rdfs:comment "Maximum number of concurent executions."@en ;
    rdfs:domain :ConcurrencyRequirement,
        :FaaSExecutionEnvironment ;
    rdfs:range xsd:positiveInteger .

:maxRAM a owl:DatatypeProperty ;
    rdfs:comment "The maximum RAM the environment can provide to a single function."@en ;
    rdfs:domain :FaaSExecutionEnvironment ;
    rdfs:range xsd:positiveInteger .

:maxTimeAllowed a owl:DatatypeProperty ;
    rdfs:comment "The maximum time in ms of execution time."@en ;
    rdfs:domain :ExecutionTimeRequirement,
        :FaaSExecutionEnvironment ;
    rdfs:range xsd:positiveInteger .

:minRAM a owl:DatatypeProperty ;
    rdfs:comment "The smallest RAM allocation for a single function."@en ;
    rdfs:domain :FaaSExecutionEnvironment ;
    rdfs:range xsd:positiveInteger .

:minRequiredRAM a owl:DatatypeProperty ;
    rdfs:comment "The minimum RAM required for normal operations."@en ;
    rdfs:domain :MemoryRequirement ;
    rdfs:range xsd:positiveInteger .

:minRequiredStorage a owl:DatatypeProperty ;
    rdfs:comment "Minimum amount of storage need for operations."@en ;
    rdfs:domain :StorageRequirement ;
    rdfs:range xsd:positiveInteger ;
    rdfs:subPropertyOf owl:topDataProperty .

:pricingModel a owl:AsymmetricProperty,
        owl:IrreflexiveProperty,
        owl:ObjectProperty ;
    rdfs:domain :FaaSExecutionEnvironment ;
    rdfs:range :PricingModel .

:programmingLanguage a owl:AsymmetricProperty,
        owl:IrreflexiveProperty,
        owl:ObjectProperty ;
    rdfs:domain :Code .

:requirements a owl:AsymmetricProperty,
        owl:IrreflexiveProperty,
        owl:ObjectProperty ;
    rdfs:domain :Application ;
    rdfs:range :Requirement .

:sourceCode a owl:DatatypeProperty ;
    rdfs:comment "The actuam source files encoded"@en ;
    rdfs:domain :Code ;
    rdfs:range xsd:base64Binary .

:supportedPackages a owl:AsymmetricProperty,
        owl:IrreflexiveProperty,
        owl:ObjectProperty ;
    rdfs:domain :FaaSExecutionEnvironment .

:CPURequirement a owl:Class ;
    rdfs:subClassOf :Requirement .

:ConcurrencyRequirement a owl:Class ;
    rdfs:subClassOf :Requirement .

:ExecutionTimeRequirement a owl:Class ;
    rdfs:subClassOf :Requirement .

:GeographicalRequirement a owl:Class ;
    rdfs:subClassOf :Requirement .

:PricingModel a owl:Class ;
    rdfs:label "Pricing Model"@en ;
    rdfs:comment "Definition of a pricing method for FaaS environments"@en .

:SLA a owl:Class ;
    rdfs:label "Service Level Agreements"@en .

wd:Q272629 rdfs:label "x86_64" .

wd:Q272683 rdfs:label "instruction set architecture" .

:MemoryRequirement a owl:Class ;
    rdfs:subClassOf :Requirement .

:Metric a owl:Class ;
    rdfs:label "Metric"@en .

:StorageRequirement a owl:Class ;
    rdfs:subClassOf :Requirement .

:Application a owl:Class ;
    rdfs:comment "Definition of a functional application, it is more then the source code, it contains also infrastructure requirements."@en .

:Code a owl:Class ;
    rdfs:label "Code"@en ;
    rdfs:comment "Source Code object" .

:Requirement a owl:Class .

:FaaSExecutionEnvironment a owl:Class ;
    rdfs:label "Function as a Service Execution Environment"@en ;
    rdfs:comment "Entity that can run function \"Function as a service\" type of capability"@en .

